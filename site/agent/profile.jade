												li
													strong Works In:
													p {{$root.agent.cities.join(', ')}}
												li
													strong Specialties:
													p {{$root.agent.specialties.join(', ')}}
												li
													strong Languages:
													p {{$root.agent.languages.join(', ')}}
							#contactDetails.tab-pane.fade.in
